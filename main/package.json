{
  "name": "electron_angular_esbuild_main",
  "version": "1.0.0",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "install-app-deps": "electron-builder install-app-deps --help",
    "postinstall": "npm run install-app-deps",
    "build": "node esbuild.mjs",
    "electron:build": "electron-builder build --publish=never",
    "watch": "node esbuild.mjs --watch",
    "start": "npm run build && wait-on tcp:4200 && electron .",
    "start:no-wait": "npm run build && electron .",
    "lint": "npm run eslint && npm run prettier",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier . --write"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/nodemon": "^1.19.6",
    "chokidar": "^3.6.0",
    "debounce-fn": "^6.0.0",
    "electron": "^29.2.0",
    "electron-builder": "^24.13.3",
    "electron-debug": "^3.2.0",
    "electron-util": "^0.18.1",
    "esbuild": "^0.20.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "prettier": "^3.2.5",
    "typescript": "~5.4.2",
    "typescript-eslint": "^7.6.0",
    "wait-on": "^7.2.0"
  }
}
